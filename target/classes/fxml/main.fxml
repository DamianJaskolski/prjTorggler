<BorderPane fx:id="mainBorderPane" prefHeight="768.0" prefWidth="1400.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="torggler.controllers.MainController">
